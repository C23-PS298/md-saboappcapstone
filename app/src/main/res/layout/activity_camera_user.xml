<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".user.camera.CameraUserActivity">

    <VideoView
        android:id="@+id/video_user_1"
        android:layout_width="390dp"
        android:layout_height="223dp"
        android:layout_marginTop="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">
    </VideoView>

    <FrameLayout
        android:id="@+id/notifkasi_camera_user"
        android:layout_width="358dp"
        android:layout_height="70dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/background_user_notifikasi"
        android:backgroundTint="@color/merah_muda"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/video_user_1">

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/notifikasi_user_logo_red"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="15dp"
            android:backgroundTint="@color/merah"
            android:contentDescription="Status"
            tools:ignore="TouchTargetSizeCheck">
        </com.google.android.material.floatingactionbutton.FloatingActionButton>

        <TextView
            android:layout_width="180dp"
            android:layout_height="20dp"
            android:layout_marginStart="70dp"
            android:layout_marginTop="24dp"
            android:text="Unable To Recognize Object"
            android:textColor="@color/black"
            android:textSize="14dp"
            android:textStyle="bold"
            tools:ignore="TextSizeCheck">
        </TextView>

        <TextView
            android:id="@+id/detected_object_1"
            android:layout_width="60dp"
            android:layout_height="20dp"
            android:layout_gravity="end"
            android:text="DETECTED"
            android:textStyle="bold"
            android:textAlignment="center"
            android:layout_marginTop="25dp"
            android:layout_marginEnd="16dp"
            tools:ignore="RtlCompat">
        </TextView>
    </FrameLayout>

    <FrameLayout
        android:id="@+id/notifikasi_2_camera_user"
        android:layout_width="358dp"
        android:layout_height="70dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/background_user_notifikasi"
        android:backgroundTint="@color/biru_muda"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/notifkasi_camera_user">

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/notifikasi_User_blue"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="15dp"
            android:backgroundTint="@color/blue_3"
            android:contentDescription="Status">
        </com.google.android.material.floatingactionbutton.FloatingActionButton>

        <TextView
            android:layout_width="180dp"
            android:layout_height="20dp"
            android:layout_marginStart="70dp"
            android:layout_marginTop="24dp"
            android:text="a Person!"
            android:textColor="@color/black"
            android:textSize="14dp"
            android:textStyle="bold"
            tools:ignore="TextSizeCheck">
        </TextView>

        <TextView
            android:id="@+id/detected_object_2"
            android:layout_width="60dp"
            android:layout_height="20dp"
            android:layout_gravity="end"
            android:layout_marginTop="25dp"
            android:layout_marginEnd="16dp"
            android:text="DETECTED"
            android:textAlignment="center"
            android:textStyle="bold"
            tools:ignore="RtlCompat">
        </TextView>
    </FrameLayout>

    <FrameLayout
        android:id="@+id/notifikasi_3_camera_user"
        android:layout_width="358dp"
        android:layout_height="70dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/background_user_notifikasi"
        android:backgroundTint="@color/biru_muda"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/notifikasi_2_camera_user">

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/notifikasi_User_blue_2"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="15dp"
            android:backgroundTint="@color/blue_3"
            android:contentDescription="Status"
            tools:ignore="TouchTargetSizeCheck">
        </com.google.android.material.floatingactionbutton.FloatingActionButton>

        <TextView
            android:layout_width="180dp"
            android:layout_height="20dp"
            android:layout_marginStart="70dp"
            android:layout_marginTop="24dp"
            android:text="MOTION DETECTED"
            android:textColor="@color/black"
            android:textSize="14dp"
            android:textStyle="bold"
            tools:ignore="TextSizeCheck">
        </TextView>

        <TextView
            android:id="@+id/detected_object_3"
            android:layout_width="60dp"
            android:layout_height="20dp"
            android:layout_gravity="end"
            android:layout_marginTop="25dp"
            android:layout_marginEnd="16dp"
            android:text="DETECTED"
            android:textAlignment="center"
            android:textStyle="bold"
            tools:ignore="RtlCompat">
        </TextView>
    </FrameLayout>

    <FrameLayout
        android:id="@+id/notifikasi_4_camera_user"
        android:layout_width="358dp"
        android:layout_height="70dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/background_user_notifikasi"
        android:backgroundTint="@color/biru_muda"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/notifikasi_3_camera_user">

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/notifikasi_User_blue_3"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="15dp"
            android:backgroundTint="@color/blue_3"
            android:contentDescription="Status"
            tools:ignore="TouchTargetSizeCheck">
        </com.google.android.material.floatingactionbutton.FloatingActionButton>

        <TextView
            android:layout_width="180dp"
            android:layout_height="20dp"
            android:layout_marginStart="70dp"
            android:layout_marginTop="24dp"
            android:text="MOTION DETECTED"
            android:textColor="@color/black"
            android:textSize="14dp"
            android:textStyle="bold"
            tools:ignore="TextSizeCheck">
        </TextView>

        <TextView
            android:id="@+id/detected_object_"
            android:layout_width="60dp"
            android:layout_height="20dp"
            android:layout_gravity="end"
            android:layout_marginTop="25dp"
            android:layout_marginEnd="16dp"
            android:text="DETECTED"
            android:textAlignment="center"
            android:textStyle="bold"
            tools:ignore="RtlCompat">
        </TextView>
    </FrameLayout>

    <com.google.android.material.bottomnavigation.BottomNavigationView
       android:id="@+id/navigation_bar_user_camera"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="?android:attr/windowBackground"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:menu="@menu/bottom_nav_menu_user"
        tools:ignore="VisualLintBottomNav">
    </com.google.android.material.bottomnavigation.BottomNavigationView>

</androidx.constraintlayout.widget.ConstraintLayout>